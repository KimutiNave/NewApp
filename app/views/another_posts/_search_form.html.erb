<div data-turbo="false">
  <%= search_form_for @q, url: another_posts_path do |f| %>
    <div data-controller="autocomplete" data-autocomplete-url-value="/another_posts/search" role="combobox">
      <%= f.search_field :status_error_name_or_file_type_file_name_or_other_file_name_or_other_error_name_or_title_or_code_content_or_other_content_cont, type: "text", data: { autocomplete_target: 'input' }, class: "form-control", placeholder: "フリーワード検索" %>
      <%= f.hidden_field :status_error_name_or_file_type_file_name_or_other_file_name_or_other_error_name_or_title, type: "hidden", data: { autocomplete_target: 'hidden' } %>
      <ul class="list-group" style="max-height: 10rem; overflow-y: scroll;" data-autocomplete-target="results"></ul>
    </div>
    <%= f.label :created_at_gteq, '投稿日から' %>
    <%= f.date_field :created_at_gteq %>
    <%= f.label :created_at_lteq, '投稿日まで' %>
    <%= f.date_field :created_at_lteq %>
    <%= f.submit '検索', class: "btn btn-success" %>
  <% end%>
</div>
